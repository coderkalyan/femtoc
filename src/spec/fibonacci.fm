let fibonacci = fn (n: i32) i32 {
    if n == 0 {
        return 1;
    } else if n == 1 {
        return 1;
    }

    let mut a = 1; // fib(0)
    let mut b = 1; // fib(1)

    for let mut i = 2; i <= n; i += 1 {
        let tmp = a;
        a = b;
        b = tmp + b;
    }
};

let main = fn() void {
    let fib = fibonacci(5);
};
