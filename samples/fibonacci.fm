let fibonacci = fn (n: i32) i32 {
    if n == 0 {
        return 1;
    } else if n == 1 {
        return 1;
    }

    let mut a: i32 = 1; // fib(0)
    let mut b: i32 = 1; // fib(1)

    for let mut i: i32 = 2; i <= n; i += 1 {
        let tmp = a;
        a = b;
        b = tmp + b;
    }

    return b;
};

let main = fn () void {
    let x: i32 = 5;
    let fib = fibonacci(x);
};
